            JOIN e_admin_user_role r ON u.id = r.uid
        </if>
        <include refid="base_where"/>
    </select>
    <sql id="base_where">
        <where>
            <if test="roleId!=null">
                AND r.rid = #{roleId}
            </if>
            <if test="platformFlag!=null">
                AND u.platform_flag = #{platformFlag}
            </if>
            <if test="keyWord!=null">
                AND ( u.nickname LIKE CONCAT( '%' , #{keyWord} , '%')
                OR u.email LIKE CONCAT('%',#{keyWord},'%') )
            </if>
            <if test="type!=null">
                AND u.type = #{type}
            </if>
            <if test="email!=null">
                AND u.email = #{email}
            </if>
            <if test="name!=null">
                AND u.nickname = #{name}
            </if>
            <if test="id!=null">
